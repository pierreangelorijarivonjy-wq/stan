{
    "version": 2,
    "builds": [
        {
            "src": "package.json",
            "use": "@vercel/static-build",
            "config": {
                "distDir": "public"
            }
        },
        {
            "src": "api/index.php",
            "use": "vercel-php@0.7.0"
        }
    ],
    "routes": [
        {
            "src": "/(build|assets|images|storage|favicon.ico|robots.txt|manifest.json|sw.js)(.*)",
            "dest": "/public/$1$2"
        },
        {
            "src": "/(.*)",
            "dest": "/api/index.php"
        }
    ],
    "env": {
        "APP_ENV": "production",
        "APP_DEBUG": "false",
        "LOG_CHANNEL": "stderr"
    }
}